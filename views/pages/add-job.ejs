<body>
    <div class="container">
        <h2>Job Manager</h2>
        <br>
        <form action="/">
            <button type= "submit" class="btn btn-info">Go to main page</button>
        </form>
        <br>
        <h4>Please fill Job details to add job into the system</h4>
        <br>
   
    <br>
    <% if(typeof message != 'undefined'){ %>
        <div class="alert alert-danger" role="alert">
            <%= message %>
        </div>
    <% }%>
    
    <row>
        <col md="4">
        <form action="/API446/addJob" method="POST">
            <div class="form-group">
                <label for="jobId"><b>Job Id</b></label>
                <input type="text" required class="form-control" name="jobId" placeholder="Please enter Id of the Job ending with 446. (e.g. j1446)">
            </div>
            <div class="form-group">
                <label for="partId"><b>Part Id</b></label>
                <input type="number" required class="form-control" name="partId" placeholder="Please enter part Id ending with 446. (e.g. 1446)">
            </div>
            <div class="form-group">
                <label for="qty"><b>Quantity</b></label>
                <input type="text" class="form-control" name="qty" placeholder="Please enter Quantity.">
            </div>
            <button type="submit" class="btn btn-success">Add Job</button>
        </form>
        </col>
    </row>
    </div>
</body>